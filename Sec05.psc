Algoritmo Sec05
	// Escribir un algoritmo que calcule el área de un rectángulo
		Definir Largo, Ancho Como Reales;
		
		Escribir "Introduzca el largo: ";
		Leer Largo;
		Escribir "Introduzca el ancho: ";
		Leer Ancho;
		
		Escribir "El área del rectángulo es: ", Largo * Ancho;
FinAlgoritmo
